{
  "name": "gm",
  "version": "2.0.4",
  "description": "Advanced Claude Code plugin with WFGY integration, MCP tools, and automated hooks",
  "author": "AnEntrypoint",
  "license": "MIT",
  "keywords": [
    "claude-code",
    "claude-plugin",
    "wfgy",
    "mcp",
    "automation",
    "glootie"
  ],
  "homepage": "https://github.com/AnEntrypoint/gm",
  "mcp": {
    "dev": {
      "command": "bunx",
      "args": [
        "mcp-glootie@latest"
      ],
      "timeout": 360000
    },
    "code-search": {
      "command": "bunx",
      "args": [
        "codebasesearch@latest"
      ],
      "timeout": 360000
    }
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "postinstall": "node -e \"const fs=require('fs'); try { fs.chmodSync('./cli.mjs', 0o755); } catch(e) {}\""
  }
}
